{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
    "blocks": [
        {
            "type": "newline"
          },
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
            {
                "type": "exit",
                "style": "plain",
                "foreground": "#C94A16",
                "properties": {
                  "prefix": "x",
                  "display_exit_code": false
                }
              },
          {
            "type": "root",
            "style": "plain",
            "foreground": "#FFEE58",
            "properties": {
              "prefix": "",
              "postfix": " <#ffffff>root in</> "
            }
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "#2FC4DE",
            "properties": {
              "style": "full",
              "prefix": ""
            }
          },
          {
            "type": "git",
            "style": "plain",
            "foreground": "#3FDA98",
            "properties": {
                "display_stash_count": true,
                "display_upstream_icon": true,
                "display_status": true,
                "status_colors_enabled": true,
                "color_background": false,
                "local_changes_color": "#ffeb3b",
                "working_color": "#E84855",
                "staging_color": "#2FDA4E",
                "ahead_color": "#2EC4B6",
                "behind_color": "#8A4FFF",
              "prefix": "<#ffffff>on</> "
            }
          },
          {
            "type": "node",
            "style": "plain",
            "foreground": "#98C379",
            "properties": {
              "display_version": true,
              "prefix": "<#ffffff>via</> \uE781 "
            }
          },
          {
              "type": "executiontime",
              "style": "plain",
              "foreground": "#ffd700",
              "properties": {
                  "style": "roundrock",
                  "threshold": 10,
                    "prefix": "<#ffffff>took</> "
              }
          }
        ]
      },
      {
        "type": "newline"
      },
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#63F08C",
            "properties": {
              "prefix": "",
              "text": "\u279C"
            }
          }
        ]
      }
    ]
  }
  